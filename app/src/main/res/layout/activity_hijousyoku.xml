<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.Hijousyoku">

    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.Hijousyoku_List"
        class="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.Hijousyoku_List"
        android:id="@+id/fragment_Hijousyoku_List"
        tools:layout="@layout/fragment_hijousyoku__list"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/fragment2" />

    <fragment
        class="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.ButtonFragment"
        tools:layout="@layout/fragment_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.ButtonFragment"
        android:id="@+id/fragment2"
        android:layout_above="@+id/fragment_ad"
        android:layout_centerHorizontal="true" />

    <fragment
        class="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.Ad"
        tools:layout="@layout/fragment_ad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="jp.co.nichiwa_system.yamashitamasaki.Sonaechao.Ad"
        android:id="@+id/fragment_ad"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
